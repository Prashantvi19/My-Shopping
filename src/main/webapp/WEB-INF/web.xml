<?xml version="1.0" encoding="UTF-8"?>
<element>	
	<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://jakarta.ee/xml/ns/jakartaee"
		xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
		id="WebApp_ID" version="6.0">
		<display-name>OnlineShopping</display-name>
		<welcome-file-list>
			<welcome-file>index.jsp</welcome-file>
		</welcome-file-list>
		<servlet>
			<description></description>
			<display-name>DB_Connection</display-name>
			<servlet-name>DB_Connection</servlet-name>
			<servlet-class>dot.java.moldel.DB_Connection</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>DB_Connection</servlet-name>
			<url-pattern>/DB_Connection</url-pattern>
		</servlet-mapping>
		<servlet>
			<description></description>
			<display-name>Singup</display-name>
			<servlet-name>Singup</servlet-name>
			<servlet-class>dot.java.servlet.Singup</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>Singup</servlet-name>
			<url-pattern>/Singup</url-pattern>
		</servlet-mapping>
		<servlet>
			<description></description>
			<display-name>login</display-name>
			<servlet-name>login</servlet-name>
			<servlet-class>dot.java.servlet.login</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>login</servlet-name>
			<url-pattern>/login</url-pattern>
		</servlet-mapping>
		<servlet>
			<description></description>
			<display-name>ForgotPassword</display-name>
			<servlet-name>ForgotPassword</servlet-name>
			<servlet-class>dot.java.servlet.ForgotPassword</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>ForgotPassword</servlet-name>
			<url-pattern>/ForgotPassword</url-pattern>
		</servlet-mapping>


		<servlet>
			<description></description>
			<display-name>AddToCart</display-name>
			<servlet-name>AddToCart</servlet-name>
			<servlet-class>dot.java.servlet.AddToCart</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>AddToCart</servlet-name>
			<url-pattern>/addpocartproduct</url-pattern>
		</servlet-mapping>


		<servlet>
			<description></description>
			<display-name>CartRemoveUpdate</display-name>
			<servlet-name>CartRemoveUpdateQuantity</servlet-name>
			<servlet-class>dot.java.servlet.CartRemoveUpdateQuantity</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>CartRemoveUpdateQuantity</servlet-name>
			<url-pattern>/userCartUpdate</url-pattern>
		</servlet-mapping>

		<servlet>
			<description></description>
			<display-name>PlaceOrder</display-name>
			<servlet-name>PlaceOrder</servlet-name>
			<servlet-class>dot.java.servlet.PlaceOrder</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>PlaceOrder</servlet-name>
			<url-pattern>/placeOrderProduct</url-pattern>
		</servlet-mapping>

	</web-app>
	
</element>
